    "import cv2\n",
    "img = cv2.imread(\"Lenna.png\",0)\n",
    "width = img.shape[1] # keep original width\n",
    "height = 450\n",
    "dim = (width, height)\n",
    "resized = cv2.resize(img, dim, interpolation = cv2.INTER_AREA)\n",
    "cv2.imshow(\"Resized image\", resized)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n"
   ]
